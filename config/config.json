{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "react-understanding-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/reactUnderstanding/ReactUnderstandingWebPart.js",
          "manifest": "./src/webparts/reactUnderstanding/ReactUnderstandingWebPart.manifest.json"
        }
      ]
    },
    "property-pane-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/propertyPaneWebpart/PropertyPaneWebpartWebPart.js",
          "manifest": "./src/webparts/propertyPaneWebpart/PropertyPaneWebpartWebPart.manifest.json"
        }
      ]
    },
    "spfx-functional-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spfxFunctionalForm/SpfxFunctionalFormWebPart.js",
          "manifest": "./src/webparts/spfxFunctionalForm/SpfxFunctionalFormWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ReactUnderstandingWebPartStrings": "lib/webparts/reactUnderstanding/loc/{locale}.js",
    "PropertyPaneWebpartWebPartStrings": "lib/webparts/propertyPaneWebpart/loc/{locale}.js",
    "SpfxFunctionalFormWebPartStrings": "lib/webparts/spfxFunctionalForm/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}